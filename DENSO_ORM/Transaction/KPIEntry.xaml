<Page x:Class="DENSO_ORM.Transaction.KPIEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DENSO_ORM.Transaction"
        mc:Ignorable="d"
        Title="KPIEntry"  Loaded="Window_Loaded" KeyDown="Window_KeyDown" Style="{StaticResource PageStyle}" Unloaded="Page_Unloaded" >
    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
        <Grid >
            <Grid VerticalAlignment="Top" HorizontalAlignment="Left"  Margin="21,0,0,0">
                <TextBlock FontWeight="Bold" FontSize="15"  x:Name="txtDatetime" Text="Date Time"  Foreground="Black"></TextBlock>
            </Grid>
            <Grid HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,1,0,0" >
                <TextBlock TextDecorations="UNDERLINE" Text="KPI ENTRY" HorizontalAlignment="Center" FontFamily="Calibri" FontSize="20"  Foreground="OrangeRed" FontWeight="ExtraBold" ></TextBlock>
            </Grid>
            <Grid HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,165,0,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="{Binding}"></ColumnDefinition>
                    <ColumnDefinition Width="{Binding}"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Border Style="{StaticResource BorderStyle}" Margin="0,0,20,0" Grid.Column="0" HorizontalAlignment="Left"   VerticalAlignment="Top">
                    <Grid Margin="7">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="90"></ColumnDefinition>
                            <ColumnDefinition Width="250"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="35">
                            </RowDefinition>
                            <RowDefinition Height="35">
                            </RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="1" Content="Line Name:" Height="Auto"   />
                        <Label Grid.Row="0" Grid.Column="0"  Content="Line Group:" Height="Auto" />
                        <TextBox Grid.Column="1" HorizontalAlignment="Left"   Width="250" Text="" x:Name="txtlinegrp" VerticalAlignment="Top"  IsReadOnly="True"/>
                        <TextBox Grid.Column="1" HorizontalAlignment="Left"  Width="250" Text="" x:Name="txtlinename" VerticalAlignment="Top"  Grid.Row="1" IsReadOnly="True"   />
                    </Grid>
                </Border>
                <Border Style="{StaticResource BorderStyle}" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top">
                    <Grid Margin="7">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="85"></ColumnDefinition>
                            <ColumnDefinition Width="355"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="35">
                            </RowDefinition>
                            <RowDefinition Height="35">
                            </RowDefinition>
                        </Grid.RowDefinitions>
                        <ComboBox Width="240" HorizontalAlignment="Left" x:Name="cmbShiftName" Grid.Column="1"  Grid.Row="1" SelectionChanged="CmbShiftName_SelectionChanged" >
                            <ComboBoxItem>First Shift</ComboBoxItem>
                            <ComboBoxItem>Second Shift</ComboBoxItem>
                            <ComboBoxItem>Third Shift</ComboBoxItem>
                        </ComboBox>
                        <DatePicker  ToolTip="From date" BorderBrush="Transparent" Width="180" HorizontalAlignment="Left" Grid.Row="0" Grid.Column="1" Name="dtpFrom"  />
                        <DatePicker ToolTip="To date"  BorderBrush="Transparent" Width="180" Margin="0,0,-6,0" HorizontalAlignment="Right" Grid.Row="0" Grid.Column="1" Name="dtpTo"   />
                        <Label Grid.Row="1" Grid.Column="0"  Content="Shift Name:"  />
                        <Label Content="Date:" Grid.Row="0" Grid.Column="0"></Label>
                        <Button ToolTip="Press Alt+B for Back" Width="100" HorizontalAlignment="Right" Grid.Column="1" Grid.Row="1"  x:Name="btnGet" Click="BtnGet_Click">
                            <TextBlock x:Name="txtGet"><Underline><Run Text="G"/></Underline><Run Text="ET"/></TextBlock>

                        </Button>

                    </Grid>
                </Border>
            </Grid>
            <Border  Style="{StaticResource BorderStyle}" Height="115" Margin="0,42,0,0" VerticalAlignment="Top" HorizontalAlignment="Center">
                <Grid Margin="7">
                    <DataGrid x:Name="dvgModelDeatils">
                        <DataGrid.Columns>
                            <DataGridTextColumn Width="auto" Header="  SLNO  " ElementStyle="{StaticResource WrapText}"  Binding="{Binding SLNO}" />
                            <DataGridTextColumn Width="auto" Header="  PRODUCT CODE " ElementStyle="{StaticResource WrapText}" Binding="{Binding PartNo}" />
                            <DataGridTextColumn Width="auto" Header="  MODEL NAME " ElementStyle="{StaticResource WrapText}" Binding="{Binding ModelName}" />
                            <DataGridTextColumn Width="auto" Header="  STR TIME " ElementStyle="{StaticResource WrapText}" Binding="{Binding STDTime}" />
                            <DataGridTextColumn Width="auto" Header="  BASE TIME " ElementStyle="{StaticResource WrapText}" Binding="{Binding BaseTime}" />
                            <DataGridTextColumn Width="auto" Header="  CYCLE TIME " ElementStyle="{StaticResource WrapText}" Binding="{Binding CyscleTime}" />
                            <DataGridTextColumn Width="auto" Header="  OK QTY " ElementStyle="{StaticResource WrapText}" Binding="{Binding OKQty}" />
                            <DataGridTextColumn Width="auto" Header="  REJ QTY " ElementStyle="{StaticResource WrapText}" Binding="{Binding RejQty}" />

                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </Border>
            <Border  Style="{StaticResource BorderStyle}" Margin="0,1540,0,5" VerticalAlignment="Stretch" HorizontalAlignment="Center">
                <Grid Margin="7">
                    <DataGrid x:Name="dvgKPIDeatils"  SelectionChanged="DvgKPIDeatils_SelectionChanged">
                        <DataGrid.Columns>
                            <DataGridTextColumn Width="auto" Header="  RefNo  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}" Binding="{Binding RefNo}" />
                            <DataGridTextColumn Width="auto" Header="  SLNO  " ElementStyle="{StaticResource WrapText}"  Binding="{Binding SLNO}" />
                            <DataGridTextColumn Width="auto" Header="  TOTAL MANPOWER GIVEN BY COMPANY " ElementStyle="{StaticResource WrapText}" Binding="{Binding [TOTAL MANPOWER GIVEN BY COMPANY]}" />
                            <DataGridTextColumn Width="auto" Header="  ABSENT " ElementStyle="{StaticResource WrapText}" Binding="{Binding [ABSENT]}" />
                            <DataGridTextColumn Width="auto" Header="  INLINE OPERATOR" ElementStyle="{StaticResource WrapText}" Binding="{Binding [INLINE OPERATOR]}" />
                            <DataGridTextColumn Width="auto" Header="  OFFLINE (LINE PARTS FEEDER) " ElementStyle="{StaticResource WrapText}" Binding="{Binding [OFFLINE (LINE PARTS FEEDER)]}" />
                            <DataGridTextColumn Width="auto" Header="  OUTLINE (LINE SUPPORTER)  "  ElementStyle="{StaticResource WrapText}" Binding="{Binding [OUTLINE (LINE SUPPORTER)]}" />
                            <DataGridTextColumn Width="auto" Header="  [DIRECT MANPOWER]  "  ElementStyle="{StaticResource WrapText}" Binding="{Binding [DIRECT MANPOWER]}" />
                            <DataGridTextColumn Width="auto" Header="  [SUPERVISOR]  "  ElementStyle="{StaticResource WrapText}" Visibility="Hidden" Binding="{Binding [SUPERVISOR]}" />
                            <DataGridTextColumn Width="auto" Header="  [INLINE MANPOWER]  "  ElementStyle="{StaticResource WrapText}"  Binding="{Binding [INLINE MANPOWER]}" />
                            <DataGridTextColumn Width="auto" Header="  [OFFLINE (LINE PARTS FEEDER) MANHOUR]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OFFLINE (LINE PARTS FEEDER) MANHOUR]}" />
                            <DataGridTextColumn Width="auto" Header="  [OUTLINE (LINE SUPPORTER) MANHOUR]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OUTLINE (LINE SUPPORTER) MANHOUR]}" />
                            <DataGridTextColumn Width="auto" Header="  [SUPERVISOR_MANHOUR]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [SUPERVISOR_MANHOUR]}" />
                            <DataGridTextColumn Width="auto" Header="  [INLINE MANPOWER TAKEN FROM BANK]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [INLINE MANPOWER TAKEN FROM BANK]}" />
                            <DataGridTextColumn Width="auto" Header="  [INLINE BORROWED AFTER MAN POWER BANKING]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [INLINE BORROWED AFTER MAN POWER BANKING]}" />
                            <DataGridTextColumn Width="auto" Header="  [OFFLINE(MIZ) MANPOWER TAKEN FROM BANK]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OFFLINE(MIZ) MANPOWER TAKEN FROM BANK]}" />
                            <DataGridTextColumn Width="auto" Header="  [OFFLINE(MIZ) BORROWED AFTER MAN POWER BANKING]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OFFLINE(MIZ) BORROWED AFTER MAN POWER BANKING]}" />
                            <DataGridTextColumn Width="auto" Header="  [OUTLINE (SUPPORTER) TAKEN FROM OTHER LINES]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OUTLINE (SUPPORTER) TAKEN FROM OTHER LINES]}" />
                            <DataGridTextColumn Width="auto" Header="  [DIRECT MP WORKED IN LINE (TL / GL / SL / QA / LOGISTICS - BELOW AM)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [DIRECT MP WORKED IN LINE (TL / GL / SL / QA / LOGISTICS - BELOW AM)]}" />
                            <DataGridTextColumn Width="auto" Header="  [INLINE MP GIVEN TO BANK]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [INLINE MP GIVEN TO BANK]}" />
                            <DataGridTextColumn Width="auto" Header="  [OFFLINE(MIZ) GIVEN TO BANK]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OFFLINE(MIZ) GIVEN TO BANK]}" />
                            <DataGridTextColumn Width="auto" Header="  [OUTLINE (SUPPORTER) LENDING]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OUTLINE (SUPPORTER) LENDING]}" />
                            <DataGridTextColumn Width="auto" Header="  [OFFLINE(MIZ) LENDING AFTER MAN POWER BANKING]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OFFLINE(MIZ) LENDING AFTER MAN POWER BANKING]}" />
                            <DataGridTextColumn Width="auto" Header="  INDIRECTWORKEDINLINE  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding INDIRECTWORKEDINLINE}" />
                            <DataGridTextColumn Width="auto" Header="  [SHIFT WORKIG HRS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [SHIFT WORKIG HRS]}" />
                            <DataGridTextColumn Width="auto" Header="  [LUNCH TIME (30 Minutes)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [LUNCH TIME (30 Minutes)]}" />
                            <DataGridTextColumn Width="auto" Header="  [TOTAL WORKING HOURS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [TOTAL WORKING HOURS]}" />
                            <DataGridTextColumn Width="auto" Header="  KY17Minutes  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding KY17Minutes}" />
                            <DataGridTextColumn Width="auto" Header="  [MORNING MEETING (5 Minutes)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [MORNING MEETING (5 Minutes)]}" />
                            <DataGridTextColumn Width="auto" Header="  [TEA BREAK (10 Minutes)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [TEA BREAK (10 Minutes)]}" />
                            <DataGridTextColumn Width="auto" Header="  [TEA BREAK (10 Minutes)1]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [TEA BREAK (10 Minutes)1]}" />
                            <DataGridTextColumn Width="auto" Header="  [5S CLOSING 5 Minutes]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [5S CLOSING 5 Minutes]}" />
                            <DataGridTextColumn Width="auto" Header="  [BREAK TIME WITHOUT LUNCH]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [BREAK TIME WITHOUT LUNCH]}" />
                            <DataGridTextColumn Width="auto" Header="  [FIXED TIME]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [FIXED TIME]}" />
                            <DataGridTextColumn Width="auto" Header="  [ON JOB TRAINING (3 DAYS-NEW ENTRY TO COMPANY)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [ON JOB TRAINING (3 DAYS-NEW ENTRY TO COMPANY)]}" />
                            <DataGridTextColumn Width="auto" Header="  [EXCESS MP AFTER MP CALCULATION]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [EXCESS MP AFTER MP CALCULATION]}" />
                            <DataGridTextColumn Width="auto" Header="  [OTHER TRAININGS (BY HR, TIE, PE, QA, MNT etc)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OTHER TRAININGS BY HR TIE PE QA MNT etc]}" />
                            <DataGridTextColumn Width="auto" Header="  [MULTISKILLING]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [MULTISKILLING]}" />
                            <DataGridTextColumn Width="auto" Header="  [ALL HANDS / PARASPARA / OTHER COMPANY ACTIVITY]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [ALL HANDS / PARASPARA / OTHER COMPANY ACTIVITY]}" />
                            <DataGridTextColumn Width="auto" Header="  [SKILL COMPETETION / QCC / BUSINESS TRIP]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [SKILL COMPETETION / QCC / BUSINESS TRIP]}" />
                            <DataGridTextColumn Width="auto" Header="  [INVENTORY]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [INVENTORY]}" />
                            <DataGridTextColumn Width="auto" Header="  [PLANNED / SPECIAL 5S]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [PLANNED / SPECIAL 5S]}" />
                            <DataGridTextColumn Width="auto" Header="  [DELAY ARRIVAL / EARLY LEAVE (PAYMENT CUT/LEAVE)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [DELAY ARRIVAL / EARLY LEAVE (PAYMENT CUT/LEAVE)]}" />
                            <DataGridTextColumn Width="auto" Header="  [PLANNED TRIAL BY PE]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [PLANNED TRIAL BY PE]}" />
                            <DataGridTextColumn Width="auto" Header="  [ENERGY SHUTDOWN (GOVT POWER ONLY)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [ENERGY SHUTDOWN (GOVT POWER ONLY)]}" />
                            <DataGridTextColumn Width="auto" Header="  [FIRE ASSEMBLY / EVACUATION DRILL]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [FIRE ASSEMBLY / EVACUATION DRILL]}" />
                            <DataGridTextColumn Width="auto" Header="  [NPD TRIALS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [NPD TRIALS]}" />
                            <DataGridTextColumn Width="auto" Header="  [MEDICAL CHECKUP / OTHER PLANNED ACTIVITY BY HR]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [MEDICAL CHECKUP / OTHER PLANNED ACTIVITY BY HR]}" />
                            <DataGridTextColumn Width="auto" Header="  [COMPANY COST SAVING ACTIVITY]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [COMPANY COST SAVING ACTIVITY]}" />
                            <DataGridTextColumn Width="auto" Header="  [KANBAN ACHIEVEMENT-EARLY COMPLETION (FIXED FOR THIS MONTH)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [KANBAN ACHIEVEMENT-EARLY COMPLETION (FIXED FOR THIS MONTH)]}" />
                            <DataGridTextColumn Width="auto" Header="  [KANBAN ACHIEVEMENT-EARLY COMPLETION BASED ON DAYS CONDITION]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [KANBAN ACHIEVEMENT-EARLY COMPLETION BASED ON DAYS CONDITION]}" />
                            <DataGridTextColumn Width="auto" Header="  [NO KANBAN DUE TO CUSTOMER NO PULL]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [NO KANBAN DUE TO CUSTOMER NO PULL]}" />
                            <DataGridTextColumn Width="auto" Header="  [PLANNED ACTIVITY BY MAINTENANCE]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [PLANNED ACTIVITY BY MAINTENANCE]}" />
                            <DataGridTextColumn Width="auto" Header="  [PLANNED ACTIVITY BY QA / 2ND QA]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [PLANNED ACTIVITY BY QA / 2ND QA]}" />
                            <DataGridTextColumn Width="auto" Header="  [VISITOR / ISO / CUSTOMER AUDIT TIME]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [VISITOR / ISO / CUSTOMER AUDIT TIME]}" />
                            <DataGridTextColumn Width="auto" Header="  [MAJOR / PLANNED CLEAN UP BEFORE LONG HOLIDAY]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [MAJOR / PLANNED CLEAN UP BEFORE LONG HOLIDAY]}" />
                            <DataGridTextColumn Width="auto" Header="  [IMPROVEMENT ACTIVITY (KAIZEN/LAYOUT CHANGE etc)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [IMPROVEMENT ACTIVITY (KAIZEN/LAYOUT CHANGE etc)]}" />
                            <DataGridTextColumn Width="auto" Header="  [PLANNED SNACKS BREAK AS PER HR POLICY]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [PLANNED SNACKS BREAK AS PER HR POLICY]}" />
                            <DataGridTextColumn Width="auto" Header="  [5S (BANK RETURNED MP)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [5S (BANK RETURNED MP)]}" />
                            <DataGridTextColumn Width="auto" Header="  [MULTISKILLING (BANK RETURNED MP)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [MULTISKILLING (BANK RETURNED MP)]}" />
                            <DataGridTextColumn Width="auto" Header="  [NON PRESENCE HRS - DIRECT (EXCLUDE HOURS)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [NON PRESENCE HRS - DIRECT (EXCLUDE HOURS)]}" />
                            <DataGridTextColumn Width="auto" Header="  [AVAIABLE WORKING (DIRECT) HRS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [AVAIABLE WORKING (DIRECT) HRS]}" />
                            <DataGridTextColumn Width="auto" Header="  [GOOD PRODUCTON HRS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [GOOD PRODUCTON HRS]}" />
                            <DataGridTextColumn Width="auto" Header="  [TOTAL LOSS HRS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [TOTAL LOSS HRS]}" />
                            <DataGridTextColumn Width="auto" Header="  [PART REJECTION (SCRAP+REWORK)]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [PART REJECTION (SCRAP+REWORK)]}" />
                            <DataGridTextColumn Width="auto" Header="  [LOSS HRS RATIO]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [LOSS HRS RATIO]}" />
                            <DataGridTextColumn Width="auto" Header="  [AVAIABLE WORKING HRS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [AVAIABLE WORKING HRS]}" />
                            <DataGridTextColumn Width="auto" Header="  [GOOD PRODUCTON HRS FOR DAY]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [GOOD PRODUCTON HRS FOR DAY]}" />
                            <DataGridTextColumn Width="auto" Header="  [TOTAL LOSS HRS FOR DAY]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [TOTAL LOSS HRS FOR DAY]}" />
                            <DataGridTextColumn Width="auto" Header="  [TOTAL LOSS HRS RATIO]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [TOTAL LOSS HRS RATIO]}" />
                            <DataGridTextColumn Width="auto" Header="  [LINE IN-CHARGE]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [LINE IN-CHARGE]}" />
                            <DataGridTextColumn Width="auto" Header="  [LINE IN-CHARGE MAN-HOURS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [LINE IN-CHARGE MAN-HOURS]}" />
                            <DataGridTextColumn Width="auto" Header="  [USED AS MANPOWER]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [USED AS MANPOWER]}" />
                            <DataGridTextColumn Width="auto" Header="  [USED AS MANHOUR]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [USED AS MANHOUR]}" />
                            <DataGridTextColumn Width="auto" Header="  [USED AS OBSERVER MANHOUR]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [USED AS OBSERVER MANHOUR]}" />
                            <DataGridTextColumn Width="auto" Header="  [KPI]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [KPI]}" />
                            <DataGridTextColumn Width="auto" Header="  [DIRECT HRS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [DIRECT HRS]}" />
                            <DataGridTextColumn Width="auto" Header="  [GOOD HRS]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [GOOD HRS]}" />
                            <DataGridTextColumn Width="auto" Header="  [DPR]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [DPR]}" />
                            <DataGridTextColumn Width="auto" Header="  [BASE GH RATIO]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [BASE GH RATIO]}" />
                            <DataGridTextColumn Width="auto" Header="  [GH AGAINST BASE]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [GH AGAINST BASE]}" />
                            <DataGridTextColumn Width="auto" Header="  [DIRECT HRS1]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [DIRECT HRS1]}" />
                            <DataGridTextColumn Width="auto" Header="  [GOOD HRS1]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [GOOD HRS1]}" />
                            <DataGridTextColumn Width="auto" Header="  [GH AGAINST BASE1]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [GH AGAINST BASE1]}" />
                            <DataGridTextColumn Width="auto" Header="  [DPR1]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [DPR1]}" />
                            <DataGridTextColumn Width="auto" Header="  [LHR]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [LHR]}" />
                            <DataGridTextColumn Width="auto" Header="  [LHR DAILY]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [LHR DAILY]}" />
                            <DataGridTextColumn Width="auto" Header="  [LEND]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [LEND]}" />
                            <DataGridTextColumn Width="auto" Header="  [BANK]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [BANK]}" />
                            <DataGridTextColumn Width="auto" Header="  [OJT]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OJT]}" />
                            <DataGridTextColumn Width="auto" Header="  [EXCLUDE]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [EXCLUDE]}" />
                            <DataGridTextColumn Width="auto" Header="  [PURE EXCLUDE]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [PURE EXCLUDE]}" />
                            <DataGridTextColumn Width="auto" Header="  [MIZ DH]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [MIZ DH]}" />
                            <DataGridTextColumn Width="auto" Header="  [OUTLINE DH]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [OUTLINE DH]}" />
                            <DataGridTextColumn Width="auto" Header="  [FQA GH]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [FQA GH]}" />
                            <DataGridTextColumn Width="auto" Header="  [INLINE MP LENDING AFTER MAN POWER BANKING]  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding [INLINE MP LENDING AFTER MAN POWER BANKING]}" />
                            <DataGridTextColumn Width="auto" Header="  SHIFT NAME  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding ShiftName}" />
                            <DataGridTextColumn Width="auto" Header="  FROM DATE  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding Fromdate}" />
                            <DataGridTextColumn Width="auto" Header="  TO DATE  " Visibility="Hidden" ElementStyle="{StaticResource WrapText}"  Binding="{Binding ToDate}" />
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </Border>

            <Border Style="{StaticResource BorderStyle}" HorizontalAlignment="Center"  Margin="0,265,0,65" VerticalAlignment="Top"  >
                <Grid Margin="10">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="{Binding}"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="{Binding}"></RowDefinition>
                        <RowDefinition Height="{Binding}"></RowDefinition>
                        <RowDefinition Height="{Binding}"></RowDefinition>
                        <RowDefinition Height="{Binding}"></RowDefinition>
                        <RowDefinition Height="{Binding}"></RowDefinition>
                        <RowDefinition Height="{Binding}"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid HorizontalAlignment="Center">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="{Binding}"></ColumnDefinition>
                            <ColumnDefinition Width="{Binding}"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <GroupBox Width="562" Margin="0,0,30,0"   Grid.Row="0" Grid.Column="0" FontWeight="DemiBold" Name="gbHeadCount"  BorderThickness="2" BorderBrush="Black" Header="HEAD COUNT" FontSize="15" Foreground="OrangeRed" FontFamily="Calibri">
                            <Grid Margin="0" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="460"/>
                                    <ColumnDefinition Width="85"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                </Grid.RowDefinitions>
